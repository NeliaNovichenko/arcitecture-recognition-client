{
    "/api/auth/*": {
      "target": "http://localhost:44357/api/auth",
      "secure": false,
      "logLevel": "debug",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api/results": "/api"
      }
    },
    "/api/recognitionResult/*": {
      "target": "http://localhost:44357/api/recognitionResult",
      "secure": false,
      "logLevel": "debug",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api/results": "/api"
      }
    },    
    "/api/user/*": {
      "target": "http://localhost:44357/api/user",
      "secure": false,
      "logLevel": "debug",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api/results": "/api"
      }
    },
    "/api/recognition/*": {
      "target": "http://localhost:5000",
      "secure": false,
      "logLevel": "debug",
      "changeOrigin": true
    }
    
}