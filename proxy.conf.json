{
    "/api/auth/*": {
      "target": "https://localhost:44357",
      "secure": false,
      "logLevel": "debug",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api/auth": "/api/auth"
      }
    },
    "/api/recognitionResult/*": {
      "target": "https://localhost:44357",
      "secure": false,
      "logLevel": "debug",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api/recognitionResult": "/api/recognitionResult"
      }
    },    
    "/api/user/*": {
      "target": "https://localhost:44357",
      "secure": false,
      "logLevel": "debug",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api/user": "/api/user"
      }
    },
    "/api/recognition/*": {
      "target": "http://localhost:5000",
      "secure": false,
      "logLevel": "debug",
      "changeOrigin": true
    }
    
}